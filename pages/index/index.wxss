.page .van-button {
  border: none;
}

.page .echartBox {
  width: 100%;
  height: 300px;
  overflow: auto;
}

.date {
  width: 100%;
}

.dateChoose {
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 30px;
  width: 100%;
}

.dateChoose .icon {
  width: 30px;
  height: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dateChoose .time {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dateChoose span {
  color: #ccc;
}
