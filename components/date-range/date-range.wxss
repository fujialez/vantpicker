.date {
  width: 100%;
}

.dateChoose {
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 30px;
  width: 100%;
}

.dateChoose .icon {
  width: 30px;
  height: 30px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.dateChoose .time {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
